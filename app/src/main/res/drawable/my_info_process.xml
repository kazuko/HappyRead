<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#f0f0f0" />
        </shape>
    </item>

    <item
        android:top="-2dp"
        android:left="-2dp"
        android:right="-2dp"
        android:bottom="0.01dp">
        <shape>
            <stroke
                android:width="0.01dp"
                android:color="#66666666" />
        </shape>
    </item>
</layer-list>